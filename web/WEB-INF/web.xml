<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>PAYMENT Project</display-name>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>	
		    classpath:payment-service.xml,		
			classpath:applicationContext-service.xml,				
			classpath:bank.xml
			classpath:b2b.xml
			classpath:prepaymentcard.xml
			classpath:export-service.xml				
		</param-value>
	</context-param>
	<filter>
		<filter-name>encodingFilter</filter-name>
		<filter-class>ebank.web.common.util.EncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
		<init-param>
			<param-name>forceEncoding</param-name>
			<param-value>false</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>encodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>payGateV2</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>axis</servlet-name>
		<servlet-class>org.apache.axis.transport.http.AxisServlet</servlet-class>
		<load-on-startup>5</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>srandNum</servlet-name>
		<servlet-class>ebank.core.common.util.PassGuardCtl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>axis</servlet-name>
		<url-pattern>/axis/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PayBack</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PreCardPayRec</url-pattern>
	</servlet-mapping>
		<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/fastPayment</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/unionPay</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PreCardNotify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PrePayAccess</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PayRec</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>		
		<url-pattern>/PayNotify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>		
		<url-pattern>/LefuPayNotify</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>		
		<url-pattern>/LefuPayRedirect</url-pattern>
	</servlet-mapping>
		<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>		
		<url-pattern>/LefuPayNotifyMobile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>		
		<url-pattern>/LefuPayRedirectMobile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PayRecGBK</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/PayRec/*.idx</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/portal</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/Portal</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/Pay</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/CompositeCashier</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/Ebank</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/tftPay/PayRec</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/bankOnline/PayRec</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/Charger</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/AccountCashier</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/SendSMS</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/fengPayNotify</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/RongxinNotify</url-pattern>
	</servlet-mapping>	
	
	
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/unionMobilePayNotify</url-pattern>
	</servlet-mapping>		
	
	
	<servlet-mapping>
		<servlet-name>payGateV2</servlet-name>
		<url-pattern>/forward</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>srandNum</servlet-name>
		<url-pattern>/srandNum</url-pattern>
	</servlet-mapping>
	<mime-mapping>
		<extension>wsdl</extension>
		<mime-type>text/xml</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>xsd</extension>
		<mime-type>text/xml</mime-type>
	</mime-mapping>
	<mime-mapping>
		<extension>msi</extension>
		<mime-type>application/x-download</mime-type>
	</mime-mapping>


	<welcome-file-list>
		<welcome-file>index_v3.jsp</welcome-file>
	</welcome-file-list>
