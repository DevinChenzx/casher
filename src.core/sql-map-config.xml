	<sqlMap resource="ebank/core/model/sqlmaps/GwViews.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwExt.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwOrders.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwTrxs.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwNotifyTx.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwService.xml"/>	
	<sqlMap resource="ebank/core/model/sqlmaps/TradePrecard.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwPayments.xml"/>
	<sqlMap resource="ebank/core/model/sqlmaps/GwFaq.xml"/>
